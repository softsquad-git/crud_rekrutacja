<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>
        {% block title %}Welcome!{% endblock %}
    </title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
    {% include 'parts/navbar.html.twig' %}

    <div class="container">
        <div class="content">
            {% block body %}{% endblock %}
        </div>
    </div>
</body>
</html>